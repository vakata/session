{
    "name": "vakata/session",
    "description": "A simple key-value storage class",
    "keywords": [
        "vakata",
        "session"
    ],
    "homepage": "https://github.com/vakata/session",
    "license": "MIT",
    "authors": [
        {
            "name": "vakata",
            "email": "github@vakata.com",
            "homepage": "http://www.vakata.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/vakata/phpdoc-md",
            "clean/phpdoc-md": "dev-master"
        }
    ],
    "require": {
        "php" : ">=8.0",
        "vakata/kvstore" : ">=1.0.2",
        "vakata/database" : "^4.0.0",
        "vakata/cache" : ">=2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "vakata\\session\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "vakata\\session\\test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "psr2": "php-cs-fixer fix ./src",
        "phpdoc": "phpdoc-md"
    }
}
